{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "NIXPACKS",
      "nixpacksPlan": {
        "phases": {
          "setup": {
            "nixPkgs": ["jdk8"]
          },
          "build": {
            "cmd": "javac chatbot/*.java"
          },
          "start": {
            "cmd": "java chatbot.chatServer"
          }
        }
      }
    }
  }